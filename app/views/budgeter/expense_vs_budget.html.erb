<p id="notice"><%= notice %></p>

<h1>Expense VS Budget</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Budget amount</th>
      <th>Actual amount</th>
      <th>Saving%</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expense_vs_budget_array.each do |b| %>
      
      
      <tr>
        <td><%= b[0] %></td>
        <td><%= b[1] %></td>
        <td><%= b[2] %></td>
        <td><%= b[3] %></td>

      </tr>
    
    <% end %>
  </tbody>
</table>

<br>

